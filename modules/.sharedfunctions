# This file defines some standard functions for use in ProxyManager modules

ParseTemplate() {
    sed 's/PMHOSTTAG/'${PM_PROXY_HOST}'/g; s/PMPORTTAG/'${PM_PROXY_PORT}'/g' $1
}

