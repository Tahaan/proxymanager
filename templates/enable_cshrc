MASTER_PROXY="PMHOSTTAG:PMPORTTAG"
setenv no_proxy "PMHOSTTAG,127.0.0.0/8,*.local"
setenv NO_PROXY "PMHOSTTAG,127.0.0.0/8,*.local"
setenv all_proxy "socks://${MASTER_PROXY}"
setenv ALL_PROXY "socks://${MASTER_PROXY}"
setenv http_proxy "http://${MASTER_PROXY}"
setenv HTTP_PROXY "http://${MASTER_PROXY}"
setenv ftp_proxy "http://${MASTER_PROXY}"
setenv FTP_PROXY "http://${MASTER_PROXY}"
setenv https_proxy "http://${MASTER_PROXY}"
setenv HTTPS_PROXY "http://${MASTER_PROXY}"
